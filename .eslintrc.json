{
    "env": {
		"es6": true,
        "browser": false,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
		"es6": true,
        "ecmaVersion": 11,
        "sourceType": "module",
		"ecmaFeatures": {
			"impliedStrict": true
		}
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["error", "double"]
    }
}
